class pair { 
  attribute int x; 
  attribute int y; 
  method pair constructor(int x, int y){
    this.x = x;
    this.y = y;
  }
}

function void main() {
  var pair p;
  var pair q;
  p = new pair(1, 2);
  p.x = 2;
  p.y = 48;
  putchar(p.y);
  q = p;
  q.y = 49;
  putchar(p.y);
  p = new pair(5, 8);
  p.y = 50;
  putchar(p.y);
  putchar(q.y);
}

// prints 0121
